#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules/lint-staged/bin/node_modules:/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules/lint-staged/node_modules:/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules:/home/tao/2TV/node_modules/.pnpm/node_modules:/home/tao/2TV/node_modules:/home/tao/node_modules:/home/node_modules:/node_modules:/home/tao/2TV/node_modules/lint-staged/bin/node_modules:/home/tao/2TV/node_modules/lint-staged/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules/lint-staged/bin/node_modules:/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules/lint-staged/node_modules:/home/tao/2TV/node_modules/.pnpm/lint-staged@12.4.1/node_modules:/home/tao/2TV/node_modules/.pnpm/node_modules:/home/tao/2TV/node_modules:/home/tao/node_modules:/home/node_modules:/node_modules:/home/tao/2TV/node_modules/lint-staged/bin/node_modules:/home/tao/2TV/node_modules/lint-staged/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
